PREFIX void: <http://rdfs.org/ns/void#>

CONSTRUCT {
    ?dataset a void:Dataset ;
        void:propertyPartition [
            void:property ?p ;
            void:entities ?entities ;
        ] .
}
#namedGraph#
WHERE {
    SELECT (COUNT(?p) AS ?entities) ?p {
        #subjectFilter#
        ?s ?p ?o .
    }
    GROUP BY ?p
}
