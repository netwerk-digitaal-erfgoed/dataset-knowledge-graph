PREFIX void: <http://rdfs.org/ns/void#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

CONSTRUCT {
    <{{dataset}}> a void:Dataset ;
        void:classPartition [
            void:class ?type ;
            void:entities ?entities ;
        ] .
} WHERE {
    SELECT (COUNT(?type) AS ?entities) ?type {
        ?s a ?type .
    }
    GROUP BY ?type
}
LIMIT 10000
