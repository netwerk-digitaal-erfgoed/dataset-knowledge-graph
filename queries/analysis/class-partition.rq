PREFIX void: <http://rdfs.org/ns/void#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

CONSTRUCT {
    <{{dataset}}> a void:Dataset;
    void:classPartition [
        void:class ?type ;
        void:entities ?total
    ] .
} WHERE {
    SELECT (COUNT(?type) as ?total) ?type {
        ?subject a ?type .
    }
    GROUP BY ?type
}
LIMIT 10000
